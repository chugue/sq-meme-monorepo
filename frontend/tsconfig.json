{
    "extends": "./.wxt/tsconfig.json",
    "compilerOptions": {
        "allowImportingTsExtensions": true,
        "jsx": "react-jsx",
        "types": [
            "react",
            "react-dom",
            "vite/client"
        ],
        "paths": {
            "@/*": [
                "./src/*"
            ]
        }
    },
    "include": [
        "src/**/*",
        "entrypoints/**/*",
        "src/injected.js"
    ]
}